{
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    "activityBar.background": "#1c212e", // 2f333d
    "activityBar.foreground": "#a9b0ba",
    "activityBarBadge.background": "#4d78cc",
    "activityBarBadge.foreground": "#a9b0ba", // f8fafd
    "badge.background": "#528BFF", // 282d34
    "badge.foreground": "#d9d9d9", // ffffff
    "button.background": "#2d3545", // 404754
    "button.foreground": "#a7b6cc", // 404754
    "debugExceptionWidget.background": "#101014",
    "debugExceptionWidget.border": "#963c47",
    "debugToolBar.background": "#1e232b",
    "debugTokenExpression.name": "#bfb993", //7dcfff 6db8e3
    "debugTokenExpression.value": "#9aa5ce",
    "debugTokenExpression.string": "#9ece6a",
    "debugTokenExpression.boolean": "#e0976c", //ff9e64
    "debugTokenExpression.number": "#e0976c", //ff9e64
    "debugTokenExpression.error": "#bb616b",
    "debugView.stateLabelBackground": "#14141b",
    "debugView.stateLabelForeground": "#787c99",
    "debugView.valueChangedHighlight": "#3d59a1aa",
    "diffEditor.insertedTextBackground": "#00809b33",
    "dropdown.background": "#181f29", // 353b45 303640 242930 21252b
    "dropdown.border": "#2d3645",
    "dropdown.foreground": "#959ca6", // b1b1b1
    "editor.background": "#0f141c", // 282d34 23282e 1d2126
    "editor.findMatchHighlightBackground": "#314365",
    "editor.focusedStackFrameHighlightBackground": "#73daca20",
    "editor.lineHighlightBackground": "#131a24", // 363f4d 293036 333b42 293036 22292e
    "editor.selectionBackground": "#343845", // 484e5b
    "editor.selectionHighlightBackground": "#0f141c", // 484e5b 23282e
    "editor.selectionHighlightBorder": "#a9a9a9",
    "editor.stackFrameHighlightBackground": "#202c3d", //  022c2e
    "editor.wordHighlightBackground": "#414959",
    "editor.wordHighlightStrongBackground": "#1c212e",
    "editorBracketMatch.border": "#8f969c",
    "editorCursor.foreground": "#a5aec2", // cfcfcf f8f8f0
    "editorError.foreground": "#c24038",
    "editorGroup.border": "#1c212e",
    "editorGroup.emptyBackground": "#0f141c", // 181a1f
    "editorGroupHeader.border": "#1c212e", // 22252e
    "editorGroupHeader.tabsBackground": "#0f141c", // 21252b
    "editorGroupHeader.tabsBorder": "#1c212e", // 22252e
    "editorHoverWidget.background": "#181f29",
    "editorHoverWidget.border": "#2d3645",
    "editorIndentGuide.background": "#1c212e",
    "editorLineNumber.activeForeground": "#6c7894",
    "editorLineNumber.foreground": "#495162",
    "editorMarkerNavigation.background": "#181f29",
    "editorOverviewRuler.border": "#1c212e",
    "editorRuler.foreground": "#1c212e",
    "editorSuggestWidget.background": "#0f141c",
    "editorSuggestWidget.border": "#1c212e",
    "editorSuggestWidget.selectedBackground": "#222833",
    "editorWhitespace.foreground": "#1c212e",
    "editorWidget.background": "#0f141c", // 21252b
    "editorWidget.resizeBorder": "#272f42",
    "foreground": "#959ca6",
    "icon.foreground": "#787c99",
    "input.background": "#1c212e", // 1d1f23
    "input.border": "#1c212e",
    "list.activeSelectionBackground": "#202936", // 2c313a 272c36 282D34 191d21
    "list.activeSelectionForeground": "#bcc3cc", // d7dae0
    "list.focusBackground": "#202936", // 383e4a
    "list.highlightForeground": "#b1b1b1", // c5c5c5
    "list.hoverBackground": "#1c212e",
    "list.hoverForeground": "#a7b6cc",
    "list.inactiveSelectionBackground": "#21242b", // 2c313a
    "list.inactiveSelectionForeground": "#b1b1b1",
    "menu.foreground": "#959ca6", // f0f0f0
    "menubar.selectionBackground": "#0f141c",
    "panel.border": "#1c212e",
    "panelTitle.activeBorder": "#959ca6",
    "panelTitle.activeForeground": "#959ca6",
    "panelTitle.inactiveForeground": "#959ca6",

    "peekViewEditor.background": "#131a24", // 151b29 101624
    "peekViewEditor.matchHighlightBackground": "#3d59a166",
    "peekViewResult.background": "#0f141c",
    "peekViewResult.fileForeground": "#808fa6",
    "peekViewResult.lineForeground": "#93c993", // 9badc7
    "peekViewResult.matchHighlightBackground": "#98a5b8",
    "peekViewResult.selectionBackground": "#3d59a133",
    "peekViewResult.selectionForeground": "#a9b1d6",
    "peekViewTitle.background": "#0f141c", // 131621 14192b
    "peekViewTitleDescription.foreground": "#787c99",
    "peekViewTitleLabel.foreground": "#a9b1d6",

    "scrollbarSlider.activeBackground": "#747d9180",
    "scrollbarSlider.background": "#4e566680",
    "scrollbarSlider.hoverBackground": "#5a637580",
    "settings.headerForeground": "#52a362", // 52a362 a35252 a3b1c4
    "sideBar.background": "#0f141c", // 21252b 1d2126
    "sideBar.border": "#1c212e", // 22252e
    "sideBar.foreground": "#959ca6",
    "sideBarSectionHeader.background": "#1c212e", // 282d34
    "sideBarSectionHeader.border": "#0f141c", // 11151a
    "statusBar.background": "#1c212e", // 21252b
    "statusBar.debuggingBackground": "#1c212e", // 7e0097
    // "statusBar.debuggingBorder": "#66017a",
    "statusBar.debuggingForeground": "#d1a660", // d3d3d3 bfbfbf
    "statusBar.foreground": "#9da5b4",
    "statusBar.noFolderBackground": "#1d2129",
    "statusBarItem.hoverBackground": "#0f141c",
    "tab.activeBackground": "#1c212e", // 2c313a
    "tab.activeForeground": "#959ca6", // ffffff
    "tab.border": "#1c212e", // 181a1f
    "tab.inactiveBackground": "#0f141c", // 21252b
    "terminal.ansiBlack": "#232833",
    "terminal.ansiBlue": "#2e8ccf",
    "terminal.ansiBrightWhite": "#959ca6", // adad9e
    "terminal.ansiGreen": "#98c379",
    "terminal.ansiWhite": "#959ca6",
    "terminal.ansiYellow": "#b4881d",
    "terminal.foreground": "#959ca6",
    "titleBar.activeBackground": "#1c212e", // 282d34
    "titleBar.activeForeground": "#959ca6",
    "titleBar.inactiveBackground": "#1c212e",
    "titleBar.inactiveForeground": "#6b717d",
    "tree.indentGuidesStroke": "#434b5e"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "foreground": "#7f7f7f",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Language methods",
      "scope": "variable.language",
      "settings": {
        "foreground": "#c5a5c5"
      }
    },
    {
      "name": "Decorators",
      "scope": "tag.decorator.js entity.name.tag.js, tag.decorator.js punctuation.definition.tag.js",
      "settings": {
        "foreground": "#79b6f2"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": "source.js constant.other.object.key.js string.unquoted.label.js",
      "settings": {
        "foreground": "#E15A60"
      }
    },
    {
      "name": "Variable",
      "scope": "variable-meta.export.default, variable.parameter, variable.other.readwrite-meta.export.default, variable, entity.name.variable",
      "settings": {
        "foreground": "#c1c1c1"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": "keyword-meta.tag.attributes, keyword.operator.new, storage.type, storage.modifier, keyword.control",
      "settings": {
        "foreground": "#c5a5c5"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": "constant.other.color, meta.tag - meta.tag.without-attributes, keyword.other.template, keyword.other.substitution",
      "settings": {
        "foreground": "#5FB3B3"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": "meta.tag.attributes - meta.embedded.expression",
      "settings": {
        "foreground": "#cecece"
      }
    },
    {
      "name": "Operator Punctuation",
      "scope": "punctuation - punctuation.definition.string.begin, punctuation.separator.key-value-punctuation.separator.key-value.css, punctuation.definition.block, punctuation.definition.parameters, punctuation.terminator.statement, punctuation.separator.comma, punctuation.definition.tag, punctuation.definition.tag.html, punctuation.definition.tag.begin.html, punctuation.definition.tag.end.html, punctuation.section.embedded, punctuation.separator.inheritance.php ",
      "settings": {
        "foreground": "#5FB3B3"
      }
    },
    {
      "name": "Operator",
      "scope": "keyword.operator - keyword.operator.assignment",
      "settings": {
        "foreground": "#c5a5c5"
      }
    },
    {
      "name": "Operator Assignment",
      "scope": "keyword.operator.assignment - meta.tag.attributes, keyword.operator.arithmetic, punctuation.separator.key-value.css",
      "settings": {
        "foreground": "#b7b7b7" // d7deea d1d8e6
      }
    },
    {
      "name": "Tag",
      "scope": "entity.name.tag, meta.tag.sgml, markup.deleted.git_gutter",
      "settings": {
        "foreground": "#e6939c" // fc929e ed959e e38d96
      }
    },
    {
      "name": "Function, Special Method, Block Level",
      "scope": "entity.name.function, support.function, variable.function, keyword.other.special-method, meta.block-level",
      "settings": {
        "foreground": "#79b6f2"
      }
    },
    {
      "name": "Meta",
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#b7b7b7"
      }
    },
    {
      "name": "React Meta Function Call",
      "scope": "meta.function-call.js - support.variable.dom.js",
      "settings": {
        "foreground": "#79b6f2"
      }
    },
    {
      "name": "React Support Variable",
      "scope": "support.variable.dom.js",
      "settings": {
        "foreground": "#c1c1c1" // d3d3d3 cfcfcf
      }
    },
    // {
    //   "name": "React Support Variable",
    //   "scope": "support.function.dom.js",
    //   "settings": {
    //     "foreground": "#79b6f2"
    //   }
    // },
    {
      "name": "Other Variable, String Link",
      "scope": "support.other.variable, string.other.link",
      "settings": {
        "foreground": "#F2777A"
      }
    },
    {
      "name": "String, Symbols, Markup Heading",
      "scope": "string, constant.other.symbol, constant.other.key, markup.heading, markup.inserted.git_gutter, meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
      "settings": {
        "foreground": "#8dc891"
      }
    },
    {
      "name": "Sub-methods",
      "scope": "entity.name.module.js, variable.import.parameter.js, variable.other.class.js",
      "settings": {
        "foreground": "#E15A60"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": "entity.name.method.js",
      "settings": {
        "foreground": "#D8DEE9"
      }
    },
    {
      "name": "meta.method.js",
      "scope": "meta.class-method.js entity.name.function.js, variable.function.constructor",
      "settings": {
        "foreground": "#D8DEE9"
      }
    },
    {
      "name": "Attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#c5a5c5"
      }
    },
    {
      "name": "Inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#8dc891"
      }
    },
    {
      "name": "Deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#E15A60"
      }
    },
    {
      "name": "Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#BB80B3"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#5FB3B3"
      }
    },
    {
      "name": "Escape Characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#5FB3B3"
      }
    },
    {
      "name": "URL",
      "scope": "*url*, *link*, *uri*",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Search Results Nums",
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "#AB7967"
      }
    },
    {
      "name": "Search Results Lines",
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#8dc891"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    },
    {
      "name": "Internal Punctuation",
      "scope": "meta.brace.round",
      "settings": {
        "foreground": "#5fb3b3"
      }
    },
    {
      "name": "Internal Variable",
      "scope": "meta.definition.variable-entity.name.function",
      "settings": {
        "foreground": "#c1c1c1" // d3d3d3 cfcfcf c9c9c9
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": "constant.numeric, constant.language, support.constant, constant.character, keyword.other.unit",
      "settings": {
        "foreground": "#a1c7e6" // #b6ccde #bdd9f0 a1c7e6 b3cee3
      }
    },
    {
      "name": "Function, Special Method, Block Level",
      "scope": "variable.other.object",
      "settings": {
        "foreground": "#9fb4c4" // 79b6f2
      }
    },
    {
      "name": "Function, Special Method, Block Level",
      "scope": "meta.export.default",
      "settings": {
        "foreground": "#9fb4c4" // 79b6f2
      }
    },
    {
      "name": "React Meta Function Call",
      "scope": "meta.function-call.js - support.variable.dom.js",
      "settings": {
        "foreground": "#9fb4c4"
      }
    },
    {
      "name": "React Support Variable",
      "scope": "support.function.dom.js",
      "settings": {
        "foreground": "#9fb4c4"
      }
    },
    {
      "scope": "text.html",
      "settings": {
        "foreground": "#b7b7b7"
      }
    },
    {
      "name": "Tag",
      "scope": "meta.tag.without-attributes, meta.embedded.expression, meta.jsx.children",
      "settings": {
        "foreground": "#b7b7b7"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": "variable.other.object.property, support.variable.property.dom",
      "settings": {
        "foreground": "#93b7f5" //#b2d0eb #c2d9ed
      }
    },
    {
      "name": "Class, Support",
      "scope": "entity.name.class, entity.other.inherited-class, entity.name.type, entity.name.type.class, support.type, support.class, support.orther.namespace.use.php, meta.use.php, support.other.namespace.php, markup.changed.git_gutter",
      "settings": {
        "foreground": "#cccc9d"
      }
    },
    // {
    //   "name": "Class, Support",
    //   "scope": "support.class",
    //   "settings": {
    //     "foreground": "#e0d18d"
    //   }
    // }
    {
      "name": "Support.Class",
      "scope": ["support.class.component", "support.variable.property.dom"],
      "settings": {
        "foreground": "#e3ba88" // #e6de9a #e3da8a #e3db98  #d9d191 #d1cb86
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#c2b68d" // c2b68d c2b486 c2b793 bfb38c
      }
    },
    {
      "scope": ["keyword.operator.assignment"],
      "settings": {
        "foreground": "#b7b7b7" // c2b68d c2b486
      }
    },
    {
      "name": "Default Embedded Color",
      "scope": [
        "punctuation.section.embedded.begin.js",
        "punctuation.section.embedded.end.js",
        "source.elixir.embedded",
        "punctuation.separator",
        "punctuation.accessor",
        "meta.brace",
        "meta.embedded",
        "string.unquoted.heredoc.ruby"
      ],
      "settings": {
        "foreground": "#b7b7b7"
      }
    }
  ]
}
